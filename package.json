{
  "name": "prisma-hapi-generator",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "bin": {
    "prisma-hapi-generator": "bin/prisma-hapi-generator"
  },
  "scripts": {
    "clean": "rm -f lib/*.js lib/util/*.js lib/routes/*.js",
    "lint": "eslint .",
    "pretest": "npm run clean",
    "test": "tap",
    "posttest": "npm run lint",
    "presnap": "npm run pretest",
    "snap": "tap"
  },
  "files": ["bin/**/*.js", "lib/**/*.js"],
  "keywords": [],
  "author": "Nathan LaFreniere <nlf@nlf.sh>",
  "license": "ISC",
  "dependencies": {
    "@prisma/generator-helper": "^4.15.0",
    "ts-morph": "^18.0.0",
    "typescript": "~5.0.3"
  },
  "devDependencies": {
    "@prisma/internals": "^4.16.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "prisma": "^4.16.0",
    "tap": "^16.3.6",
    "ts-node": "^10.9.1"
  },
  "tap": {
    "coverage": true,
    "ts": true,
    "test-env": "NODE_ENV=test",
    "files": ["test/*.ts", "test/util/*.ts"],
    "node-arg": ["-r", "ts-node/register"],
    "nyc-arg": ["--exclude", "tap-snapshots"]
  }
}
